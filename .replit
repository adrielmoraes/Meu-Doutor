modules = ["nodejs-20", "python-3.11", "bash", "web", "postgresql-16"]
[agent]
expertMode = true
integrations = ["resend:1.0.0"]

[nix]
channel = "stable-25_05"

[deployment]
deploymentTarget = "vm"
run = ["bash", "start-production.sh"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Frontend - Next.js"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Avatar AI Agent"

[[workflows.workflow]]
name = "Frontend - Next.js"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow]]
name = "Avatar AI Agent"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd livekit-agent && python run-agent.py"

[workflows.workflow.metadata]
outputType = "console"

